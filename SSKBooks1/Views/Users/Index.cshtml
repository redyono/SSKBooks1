@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

@{
    ViewData["Title"] = "Users";
}

<h2>Registered Users</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Email</th>
            <th>User Name</th>
            <th>Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.UserName</td>
                <td>@user.Id</td>
            </tr>
        }
    </tbody>
</table>
